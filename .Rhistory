lista_dfs <- list(nac05,nac06,nac07,nac08,nac09,nac10,nac11,nac12,nac13,nac14,nac15,nac16,nac17,nac18,nac19,nac20,nac21,nac22)
# Función para pegar dataframes de una lista uno sobre otro
pegar_dataframes <- function(lista_dfs) {
# Verifica que la lista no esté vacía
if (length(lista_dfs) == 0) {
stop("La lista de dataframes está vacía")
}
# Usa Reduce para combinar todos los dataframes con rbind
df_combinado <- Reduce(function(x, y) rbind(x, y), lista_dfs)
return(df_combinado)
}
